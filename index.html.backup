        // State
        const state = {
            activeTab: 'news',
            newsFilter: 'all',
            skillFilter: 'all'
        };

        // DOM
        const els = {
            tabs: document.querySelectorAll('.tab-btn'),
            sections: document.querySelectorAll('.view-section'),
            newsGrid: document.getElementById('news-grid'),
            skillsGrid: document.getElementById('skills-grid'),
            skillFilters: document.getElementById('skill-filters'),
            filterChips: document.querySelectorAll('.filter-chip')
        };

        // --- RENDERERS ---

        function renderNewsGrid() {
            els.newsGrid.innerHTML = '';
            const allItems = [];

            // 1. News Items
            db.news.news_items.forEach(item => {
                allItems.push({
                    type: 'news',
                    date: item.publish_date,
                    data: item
                });
            });

            // 2. Use Cases & Showcase
            db.showcase.use_cases.forEach(item => {
                allItems.push({ type: 'showcase', date: '2026-01-01', data: item }); // Mock date for sort
            });

             // 3. Code (optional to mix in, but maybe keep separate? For now mixing all non-code)
            // Let's filter based on state.newsFilter
            
            let filtered = allItems;
            if (state.newsFilter === 'news') filtered = allItems.filter(i => i.type === 'news');
            if (state.newsFilter === 'showcase') filtered = allItems.filter(i => i.type === 'showcase');
            if (state.newsFilter === 'code') {
                // Special case for code
                renderCodeSnippets();
                return; 
            }

            // Render
            filtered.forEach(item => {
                const card = document.createElement('article');
                card.className = 'unified-card';
                
                const d = item.data;
                const isNews = item.type === 'news';
                
                // Image handling
                let imgUrl = d.image || 'https://via.placeholder.com/400x200';
                
                const badgeText = isNews ? d.platform : 'Use Case';
                
                card.innerHTML = `
                    <div class="card-badge">${badgeText}</div>
                    <div class="card-media">
                        <img src="${imgUrl}" alt="${d.title}" loading="lazy">
                    </div>
                    <div class="card-content">
                        ${isNews ? `<div class="card-date">üìÖ ${d.publish_date}</div>` : ''}
                        <h3 class="card-title">${d.title}</h3>
                        <p class="card-summary">${d.description || d.summary}</p>
                        <div class="card-footer">
                            <div class="tag-group">
                                ${(d.tags || [d.category]).slice(0,3).map(t => `<span class="mini-tag">#${t}</span>`).join('')}
                            </div>
                            <a href="${d.source_url}" target="_blank" class="action-btn">
                                Open Link <span>‚Üí</span>
                            </a>
                        </div>
                    </div>
                `;
                els.newsGrid.appendChild(card);
            });
        }

        function renderCodeSnippets() {
            els.newsGrid.innerHTML = '';
            db.showcase.code_snippets.forEach(item => {
                const card = document.createElement('div');
                card.className = 'unified-card';
                card.innerHTML = `
                    <div class="code-preview">
                        <pre><code>${escapeHtml(item.code)}</code></pre>
                    </div>
                    <div class="card-content" style="flex: 0;">
                        <h3 class="card-title" style="font-size: 1rem;">${item.icon} ${item.title}</h3>
                        <p class="card-summary" style="font-size: 0.85rem;">${item.description}</p>
                    </div>
                `;
                els.newsGrid.appendChild(card);
            });
        }

        function renderSkillsGrid() {
            // 1. Filters
            if(els.skillFilters.innerHTML.trim() === '') {
                els.skillFilters.innerHTML = `<button class="filter-chip active" onclick="filterSkills('all', this)">All</button>`;
                db.skills.categories.forEach(cat => {
                    els.skillFilters.innerHTML += `<button class="filter-chip" onclick="filterSkills('${cat.name}', this)">${cat.icon} ${cat.name}</button>`;
                });
            }

            // 2. Content
            els.skillsGrid.innerHTML = '';
            const items = state.skillFilter === 'all' 
                ? db.skills.skills 
                : db.skills.skills.filter(s => s.category === state.skillFilter);

            items.forEach(skill => {
                const card = document.createElement('div');
                card.className = 'skill-card';
                card.setAttribute('data-cat', skill.category);
                
                const featuresHtml = skill.features.slice(0, 3)
                    .map(f => `<li class="feature-item"><span class="check-icon">‚úì</span> ${f}</li>`)
                    .join('');

                card.innerHTML = `
                    <div class="glass-overlay"></div>
                    <div class="skill-content-wrapper">
                        <div class="skill-header">
                            <div class="skill-icon">${skill.icon}</div>
                            <div class="skill-title-group">
                                <h3>${skill.title}</h3>
                                <span class="skill-name-en">${skill.name}</span>
                            </div>
                        </div>
                        <p class="skill-desc">${skill.description}</p>
                        
                        <ul class="feature-list">
                            ${featuresHtml}
                        </ul>

                        <div class="skill-actions">
                            <div class="install-box" onclick="copyText('${skill.install_cmd}', this)">
                                <span>$ ${skill.install_cmd}</span>
                                <span class="copy-icon">üìã</span>
                            </div>
                            <a href="${skill.github_url}" target="_blank" class="gh-btn">
                                <svg height="20" viewBox="0 0 16 16" version="1.1" width="20" aria-hidden="true" style="fill:currentColor"><path d="M8 0C3.58 0 0 3.58 0 8c0 3.54 2.29 6.53 5.47 7.59.4.07.55-.17.55-.38 0-.19-.01-.82-.01-1.49-2.01.37-2.53-.49-2.69-.94-.09-.23-.48-.94-.82-1.13-.28-.15-.68-.52-.01-.53.63-.01 1.08.58 1.23.82.72 1.21 1.87.87 2.33.66.07-.52.28-.87.51-1.07-1.78-.2-3.64-.89-3.64-3.95 0-.87.31-1.59.82-2.15-.08-.2-.36-1.02.08-2.12 0 0 .67-.21 2.2.82.64-.18 1.32-.27 2-.27.68 0 1.36.09 2 .27 1.53-1.04 2.2-.82 2.2-.82.44 1.1.16 1.92.08 2.12.51.56.82 1.27.82 2.15 0 3.07-1.87 3.75-3.65 3.95.29.25.54.73.54 1.48 0 1.07-.01 1.93-.01 2.2 0 .21.15.46.55.38A8.013 8.013 0 0016 8c0-4.42-3.58-8-8-8z"></path></svg>
                            </a>
                        </div>
                    </div>
                `;
                els.skillsGrid.appendChild(card);
            });
        }

        // --- ACTIONS ---

        function setTab(tabName) {
            state.activeTab = tabName;
            
            // Update Nav
            els.tabs.forEach(t => t.classList.remove('active'));
            document.querySelector(`.tab-btn[onclick="setTab('${tabName}')"]`).classList.add('active');

            // Update View
            els.sections.forEach(s => s.classList.remove('active'));
            if(tabName === 'news') document.getElementById('news-section').classList.add('active');
            else document.getElementById('skills-section').classList.add('active');
        }

        function filterNews(type, btn) {
            state.newsFilter = type;
            document.querySelectorAll('#news-section .filter-chip').forEach(b => b.classList.remove('active'));
            btn.classList.add('active');
            renderNewsGrid();
        }

        function filterSkills(cat, btn) {
            state.skillFilter = cat;
            document.querySelectorAll('#skills-section .filter-chip').forEach(b => b.classList.remove('active'));
            btn.classList.add('active');
            renderSkillsGrid();
        }

        function copyText(text, el) {
            navigator.clipboard.writeText(text).then(() => {
                const original = el.innerHTML;
                el.innerHTML = '<span>Copied!</span> <span>‚úÖ</span>';
                setTimeout(() => el.innerHTML = original, 1500);
            });
        }

        function escapeHtml(text) {
            return text.replace(/&/g, "&amp;").replace(/</g, "&lt;").replace(/>/g, "&gt;");
        }

        // --- INIT ---
        renderNewsGrid();
        renderSkillsGrid();

    </script>
</body>
</html>